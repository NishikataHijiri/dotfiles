export GREP_COLOR='10;37;41'

# PATH CONFIG
export PATH=/usr/local/bin:$PATH
export PATH=/usr/local/sbin:$PATH
export PATH=/Users/hijiri/.vim/latex:$PATH 
export PATH=$PATH:/Users/hijiri/.nodebrew/current/bin

# MY ALIASES TO WORKSPACE
export desk=/Users/hijiri/Documents/workspace 
export work=/Users/hijiri/Public/OneDrive
export study=/Users/hijiri/Public/OneDrive/workstation/development/Studying

export share_juno=/Volumes/FOLIO/Documents/Duho
export share_duho=/Volumes/FOLIO/Documents/Juno

# BIND MY ENV
export home=/Users/hijiri/
export root=/private/var/root/

export custom_vim=~/.vim/custom/

export NVM_DIR=~/.nvm
export ZSH_HIGHLIGHT_HIGHLIGHTERS_DIR=/usr/local/share/zsh-syntax-highlighting/highlighters
export apacheconfig=/usr/local/etc/apache2/2.4/

# OpenSSH
export SSH_AUTH_SOCK=$(launchctl getenv SSH_AUTH_SOCK)

# Nodebrew
export PATH=$HOME/.nodebrew/current/bin:$PATH
export NVM_DIR=~/.nvm


export EDITOR=vim

# Microsoft aspnet
export MONO_GAC_PREFIX="/usr/local"
